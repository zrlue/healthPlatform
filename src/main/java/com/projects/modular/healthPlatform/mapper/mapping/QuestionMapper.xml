<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.projects.modular.healthPlatform.mapper.QuestionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.projects.modular.healthPlatform.entity.Question">
        <id column="question_id" property="questionId" />
        <result column="context" property="context" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="user_id" property="userId" />
        <result column="pid" property="pid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        question_id AS "questionId", context AS "context", status AS "status", create_time AS "createTime", user_id AS "userId", pid AS "pid"
    </sql>

</mapper>
