<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.projects.modular.healthPlatform.mapper.HealthyServiceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.projects.modular.healthPlatform.entity.HealthyService">
        <id column="service_id" property="serviceId" />
        <result column="name" property="name" />
        <result column="intro" property="intro" />
        <result column="cover" property="cover" />
        <result column="price" property="price" />
        <result column="tel" property="tel" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        service_id AS "serviceId", name AS "name", intro AS "intro", cover AS "cover", price AS "price", tel AS "tel"
    </sql>

</mapper>
