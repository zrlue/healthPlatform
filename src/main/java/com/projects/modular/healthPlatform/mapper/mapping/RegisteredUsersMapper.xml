<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.projects.modular.healthPlatform.mapper.RegisteredUsersMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.projects.modular.healthPlatform.entity.RegisteredUsers">
        <id column="user_id" property="userId" />
        <result column="name" property="name" />
        <result column="pass_word" property="passWord" />
        <result column="user_name" property="userName" />
        <result column="pic" property="pic" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id AS "userId", name AS "name", pass_word AS "passWord", user_name AS "userName", pic AS "pic"
    </sql>

</mapper>
