<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.projects.modular.healthPlatform.mapper.RecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.projects.modular.healthPlatform.entity.Record">
        <id column="record_id" property="recordId" />
        <result column="user_id" property="userId" />
        <result column="time" property="time" />
        <result column="record" property="record" />
        <result column="report" property="report" />
        <result column="ope_id" property="opeId" />
        <result column="advise" property="advise" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        record_id AS "recordId", user_id AS "userId", time AS "time", record AS "record", report AS "report", ope_id AS "opeId", advise AS "advise"
    </sql>

</mapper>
